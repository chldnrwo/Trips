<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.trips.mapper.admin.AdminMapper">

<select id="select" resultType="com.trips.domain.jjhMember.jjhMemberDto">
SELECT 
	M_ID,
	M_NAME,
	M_GENDER,
	M_PHONE,
	M_HOST_REQUEST
FROM 
	MEMBER
WHERE
	M_HOST_REQUEST = false

</select>

</mapper>