<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.trips.mapper.ydsBoardMapper.ydsBoardMapper">
<select id="getBoardList" resultType="com.trips.domain.ydsBoard.tripsBoardDto">
SELECT
	BOARD_TITLE,
	BOARD_CONTENT,
	ORIGINAL_FILENAME,
	READCOUNT,
	STATUS,
	CREATE_DATE
FROM ACTI_BOARD;
</select>


</mapper>